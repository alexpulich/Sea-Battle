<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.ifmo.practice.seabattle.db.Match" table="Game" catalog="SEABATTLE">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="loser_id" type="java.lang.Integer">
            <column name="loser_id" not-null="true"/>
        </property>
        <property name="winner_id" type="java.lang.Integer">
            <column name="winner_id" not-null="true"/>
        </property>
        <property name="winner_eff" type="java.lang.Integer">
            <column name="winner_eff" not-null="true" default="0"/>
        </property>
        <property name="loser_eff" type="java.lang.Integer">
            <column name="loser_eff" not-null="true" default="0"/>
        </property>
        <property name="loser_nickname" type="java.lang.String">
            <column name="loser_nickname" length="20" not-null="true"/>
        </property>
        <property name="winner_nickname" type="java.lang.String">
            <column name="winner_nickname" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>