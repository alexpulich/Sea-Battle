<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.ifmo.practice.seabattle.db.Match" table="Game" catalog="SEABATTLE">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
       <property name="winnerId" type="java.lang.Integer">
           <column name="winner_id" not-null="true"/>
       </property>
        <property name="loserId" type="java.lang.Integer">
            <column name="loser_id" not-null="true"/>
        </property>
        <property name="winnerEfficiency" type="java.lang.Integer">
            <column name="winner_efficiency" not-null="true"/>
        </property>
        <property name="loserEfficiency" type="java.lang.Integer">
            <column name="loser_efficiency" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>